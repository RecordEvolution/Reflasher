{
  "name": "reflasher",
  "version": "2.0.1",
  "description": "An efficient, modern and reliable flashing tool for IoT applications",
  "author": "Record Evolution GmbH",
  "publisher": "Record Evolution GmbH",
  "main": "./out/main/index.js",
  "license": "MIT",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "postinstall": "node windows.js && electron-builder install-app-deps",
    "dev": "electron-vite dev",
    "release": "electron-builder",
    "build": "npm run typecheck && electron-vite build",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@mdi/font": "^7.3.67",
    "@mdi/js": "^7.3.67",
    "ansi-to-html": "^0.7.2",
    "drivelist": "^11.1.0",
    "echarts": "^5.4.3",
    "electron-log": "^5.0.1",
    "electron-updater": "^6.1.7",
    "etcher-sdk": "^9.0.0",
    "fix-path": "^3.0.0",
    "humanize-duration": "^3.31.0",
    "lodash": "^4.17.21",
    "mountutils": "^1.3.21",
    "node-fetch": "^2.7.0",
    "node-powershell": "^5.0.1",
    "node-wifi": "^2.0.16",
    "path-browserify": "^1.0.1",
    "pinia": "^2.1.7",
    "pretty-bytes": "^6.1.1",
    "roboto-fontface": "^0.10.0",
    "semver": "^7.5.4",
    "sudo-prompt": "^9.2.1",
    "terminate": "^2.6.1",
    "uuid": "^9.0.1",
    "vue-composable": "^1.0.0-beta.24",
    "vue-echarts": "^6.6.2",
    "vue-i18n": "^9.8.0",
    "vue-svg-gauge": "^1.2.1",
    "vuetify": "^3.4.4"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/rebuild": "^3.4.1",
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.17.5",
    "@types/node-fetch": "^2.6.9",
    "@types/node-wifi": "^2.0.2",
    "@types/path-browserify": "^1.0.2",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "electron": "^19.1.9",
    "electron-builder": "^24.6.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-vite": "^1.0.27",
    "eslint": "^8.47.0",
    "eslint-plugin-vue": "^9.17.0",
    "less": "^4.2.0",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.8"
  }
}
